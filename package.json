{
  "name": "ithinku",
  "version": "1.0.0",
  "description": "ESLint configurations and utilities for modern JavaScript/TypeScript projects",
  "private": true,
  "scripts": {
    "test:expr": "pnpm -F expr test",
    "build:expr": "pnpm -F expr build",
    "lint:expr": "pnpm -F expr lint",
    "lint:shared": "pnpm -F shared lint",
    "lint": "eslint packages",
    "format": "eslint --fix packages",
    "release": "npx lerna publish --conventional-commits"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Protagonistss/ithinku.git"
  },
  "keywords": [
    "eslint",
    "typescript",
    "javascript",
    "vue",
    "react",
    "config"
  ],
  "author": "huangshan <protagonisths@gmail.com> (https://github.com/Protagonistss)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Protagonistss/ithinku/issues"
  },
  "homepage": "https://github.com/Protagonistss/ithinku#readme",
  "devDependencies": {
    "@ithinku/eslint-config-base": "workspace:*",
    "@ithinku/eslint-config-ts": "workspace:*",
    "@ithinku/tsconfig": "workspace:*",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.11.19",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "lerna": "^4.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.0.0"
  }
}