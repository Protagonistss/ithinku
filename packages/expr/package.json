{
  "name": "@ithinku/expr",
  "version": "0.1.0",
  "description": "A TypeScript expression parser library",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "debug": "node --loader ts-node/esm --experimental-specifier-resolution=node src/index.ts"
  },
  "keywords": [
    "expression",
    "parser",
    "typescript",
    "ast"
  ],
  "author": "huangshan <protagonisths@gmail.com> (https://github.com/Protagonistss)",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.27",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}