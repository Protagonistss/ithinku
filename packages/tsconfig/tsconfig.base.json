{
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": false,
    "skipLibCheck": true,
    "newLine": "lf",
    "pretty": true,
    "isolatedModules": true,
    "noImplicitAny": true
  }
}
